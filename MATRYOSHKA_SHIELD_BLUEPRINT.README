            background-size: 40px 40px;
        }
        
        .header {
            background: rgba(10, 14, 39, 0.95);
            padding: 20px;
            border-bottom: 3px solid #00ffff;
            position: sticky;
            top: 0;
            z-index: 1000;
            backdrop-filter: blur(10px);
        }
        
        .title {
            font-size: 28px;
            text-align: center;
            letter-spacing: 4px;
            text-shadow: 0 0 15px #00ffff;
            margin-bottom: 10px;
        }
        
        .subtitle {
            text-align: center;
            font-size: 13px;
            color: #ffd700;
            letter-spacing: 2px;
        }
        
        .patent-notice {
            position: absolute;
            top: 25px;
            right: 30px;
            background: #ff6b6b;
            color: #0a0e27;
            padding: 8px 20px;
            font-weight: bold;
            font-size: 11px;
            letter-spacing: 2px;
            animation: pulse 2s infinite;
        }
        
        @keyframes pulse {
            0%, 100% { opacity: 1; box-shadow: 0 0 20px #ff6b6b; }
            50% { opacity: 0.7; box-shadow: 0 0 10px #ff6b6b; }
        }
        
        .content-section {
            max-width: 1600px;
            margin: 40px auto;
            padding: 0 20px;
        }
        
        .section-title {
            font-size: 20px;
            color: #ffd700;
            border-bottom: 2px solid #00ffff;
            padding-bottom: 10px;
            margin-bottom: 30px;
            letter-spacing: 2px;
        }
        
        .main-drawing {
            background: rgba(10, 14, 39, 0.8);
            border: 3px solid #00ffff;
            padding: 30px;
            margin-bottom: 40px;
            box-shadow: 0 0 30px rgba(0, 255, 255, 0.3);
        }
        
        .drawing-title {
            text-align: center;
            font-size: 16px;
            color: #ffd700;
            margin-bottom: 20px;
            letter-spacing: 2px;
        }
        
        svg {
            width: 100%;
            height: auto;
        }
        
        .shell-layer {
            fill: none;
            stroke: #00ffff;
            stroke-width: 2;
            transition: all 0.3s;
        }
        
        .shell-layer:hover {
            stroke: #ffd700;
            stroke-width: 3;
            filter: drop-shadow(0 0 10px #ffd700);
        }
        
        .geodesic-line {
            stroke: #00ffff;
            stroke-width: 1;
            opacity: 0.6;
        }
        
        .rotation-indicator {
            stroke: #ff6b6b;
            stroke-width: 2;
            fill: none;
            stroke-dasharray: 5, 5;
        }
        
        .armillary-ring {
            fill: none;
            stroke: #ffd700;
            stroke-width: 2;
            opacity: 0.8;
        }
        
        .dimension-line {
            stroke: #00ffff;
            stroke-width: 1;
            stroke-dasharray: 3, 3;
        }
        
        .dimension-text {
            fill: #00ffff;
            font-size: 11px;
            font-family: 'Courier New', monospace;
        }
        
        .label {
            fill: #00ffff;
            font-size: 10px;
            font-family: 'Courier New', monospace;
        }
        
        .callout-line {
            stroke: #ff6b6b;
            stroke-width: 1;
            fill: none;
        }
        
        .specs-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
            gap: 20px;
            margin-bottom: 40px;
        }
        
        .spec-panel {
            background: rgba(10, 14, 39, 0.8);
            border: 2px solid #00ffff;
            padding: 20px;
            transition: all 0.3s;
        }
        
        .spec-panel:hover {
            border-color: #ffd700;
            box-shadow: 0 0 20px rgba(255, 215, 0, 0.3);
        }
        
        .spec-panel-title {
            font-size: 14px;
            color: #ffd700;
            border-bottom: 1px solid #00ffff;
            padding-bottom: 10px;
            margin-bottom: 15px;
            font-weight: bold;
        }
        
        .spec-item {
            margin-bottom: 10px;
            font-size: 11px;
            line-height: 1.6;
        }
        
        .spec-label {
            color: #ff6b6b;
            font-weight: bold;
            display: inline-block;
            width: 140px;
        }
        
        .spec-value {
            color: #00ffff;
        }
        
        .rotating {
            animation: rotate 20s linear infinite;
            transform-origin: center;
        }
        
        @keyframes rotate {
            from { transform: rotate(0deg); }
            to { transform: rotate(360deg); }
        }
        
        .exploded-view {
            background: rgba(10, 14, 39, 0.9);
            border: 2px solid #ffd700;
            padding: 30px;
            margin-bottom: 40px;
        }
        
        .formula-box {
            background: rgba(0, 255, 255, 0.05);
            border: 1px solid #00ffff;
            padding: 15px;
            margin: 20px 0;
            font-size: 12px;
            line-height: 1.8;
        }
        
        .warning-box {
            background: rgba(255, 107, 107, 0.1);
            border: 2px solid #ff6b6b;
            padding: 15px;
            margin: 20px 0;
            font-size: 11px;
        }
        
        .warning-box::before {
            content: '‚ö† CRITICAL DESIGN PARAMETER';
            display: block;
            font-weight: bold;
            color: #ff6b6b;
            margin-bottom: 10px;
        }
        
        .assembly-sequence {
            background: rgba(10, 14, 39, 0.8);
            border: 2px solid #00ffff;
            padding: 20px;
            margin-bottom: 40px;
        }
        
        .step {
            margin-bottom: 20px;
            padding: 15px;
            background: rgba(0, 255, 255, 0.05);
            border-left: 3px solid #ffd700;
        }
        
        .step-number {
            color: #ffd700;
            font-size: 16px;
            font-weight: bold;
            margin-bottom: 8px;
        }
        
        .glow {
            filter: drop-shadow(0 0 5px currentColor);
        }
        
        table {
            width: 100%;
            border-collapse: collapse;
            margin: 20px 0;
            font-size: 11px;
        }
        
        th {
            background: rgba(0, 255, 255, 0.1);
            border: 1px solid #00ffff;
            padding: 10px;
            text-align: left;
            color: #ffd700;
        }
        
        td {
            border: 1px solid #00ffff;
            padding: 10px;
            color: #00ffff;
        }
        
        tr:hover {
            background: rgba(0, 255, 255, 0.05);
        }
    </style>
</head>
<body class="blueprint-grid">
