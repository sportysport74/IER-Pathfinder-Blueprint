pythondef oxygen_spike_signature(throat_radius, duration):
    """Calculate detectable Oâ‚‚ signature"""
    
    # Atmospheric flow rate through throat
    pressure_diff = 101325  # Pa (1 atm)
    throat_area = 3.14159 * (throat_radius ** 2)
    
    # Airflow (simplified)
    flow_rate = pressure_diff * throat_area * (1 / 1.225)  # kg/s
    
    # Oxygen mass flow
    O2_fraction = 0.21
    O2_flow = flow_rate * O2_fraction
    
    # Total oxygen displaced
    total_O2 = O2_flow * duration
    
    # Isotope ratio shift (detectable by VATT)
    isotope_shift = 0.003  # 0.3% (from time displacement)
    
    # Detection threshold
    detection_limit = 1e-6  # Parts per million
    signal_strength = isotope_shift / detection_limit
    
    return {
        'flow_rate_kg_s': O2_flow,
        'total_kg': total_O2,
        'isotope_shift': isotope_shift,
        'detectability': signal_strength
    }

